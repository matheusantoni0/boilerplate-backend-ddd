{
  "name": "api-alugamais-node-ts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "yarn husky install",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:debug": "cross-env NODE_ENV=development ts-node-dev --inspect=0.0.0.0:9229 --respawn --transpile-only src/main/server.ts",
    "build": "yarn rimraf dist && yarn tsc -p tsconfig-build.json",
    "start": "cross-env NODE_ENV=production node dist/main/server.js",
    "test:setup": "yarn migration:revert:all && yarn migration:run:test",
    "test": "yarn test:setup && cross-env NODE_ENV=test jest --passWithNoTests --silent --noStackTrace --runInBand --detectOpenHandles",
    "test:verbose": "yarn test:setup && cross-env NODE_ENV=test jest --passWithNoTests --runInBand",
    "test:unit": "yarn test:verbose --watch -c jest-unit.config.js",
    "test:integration": "yarn test:verbose --watch -c jest-integration.config.js",
    "test:staged": "yarn test --findRelatedTests",
    "test:ci": "yarn test --coverage",
    "before-commit": "lint-staged",
    "migration:show": "yarn ts-node-dev ./node_modules/typeorm/cli.js migration:show -d src/main/database/typeorm.ts",
    "migration:run": "yarn ts-node-dev ./node_modules/typeorm/cli.js migration:run -d ./src/main/database/typeorm.ts",
    "migration:prod": "cross-env NODE_ENV=production yarn typeorm migration:run -d ./dist/main/database/typeorm.js",
    "migration:revert": "yarn ts-node-dev ./node_modules/typeorm/cli.js migration:revert -d ./src/main/database/typeorm.ts",
    "migration:revert:all": "cross-env NODE_ENV=test yarn ts-node-dev revert-all-migrations.ts",
    "migration:run:test": "cross-env NODE_ENV=test yarn ts-node-dev ./node_modules/typeorm/cli.js migration:run -d ./src/main/database/typeorm.ts",
    "postinstall": "yarn build",
    "lint": "npx eslint src",
    "lint:fix": "yarn lint -- --fix",
    "prettier": "npx prettier src --check",
    "prettier:fix": "yarn prettier -- --write",
    "format": "yarn prettier:fix && yarn lint:fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
